@using (Html.BeginForm("verificar", "Verificar_titular", FormMethod.Post, new { @class = "class_verifica_cedula" }))
{
    @Html.ValidationSummary(true)
    <fieldset>
        @*<legend>Verific_client_titular</legend>*@

        <div class="editor-label">
            @Html.LabelFor(model => model.cedula_titular)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.cedula_titular)
            @Html.ValidationMessageFor(model => model.cedula_titular)
        </div>

        <p>
            <input type="submit" value="Verificar Cedula del Titular" id="verificar_titular" />
        </p>
    </fieldset>
}

